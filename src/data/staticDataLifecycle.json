        { "id": "kyc-e3", "source": "kyc-sanctions", "target": "kyc-adverse" },
        { "id": "kyc-e4", "source": "kyc-pep", "target": "kyc-adverse" },
        { "id": "kyc-e5", "source": "kyc-adverse", "target": "kyc-review" },
        { "id": "kyc-e6", "source": "kyc-review", "target": "kyc-end" }
      ]
    },
    "distribution": {
