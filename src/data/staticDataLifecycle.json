{
  "nodes": [
    {
      "id": "start",
      "type": "event",
      "label": "Client Onboarding Request",
      "icon": "play",
      "details": {
        "description": "New corporate client onboarding request received from Sales or Relationship Manager",
        "requirements": ["Client legal name", "Contact information", "Business justification"],
        "effort": "N/A - Trigger event",
        "owner": "Relationship Manager"
      }
    },
    {
      "id": "collect",
      "type": "manual",
      "label": "Collect Client Data",
      "icon": "user",
      "details": {
        "description": "Gather all required static data from the client including legal entity information, tax IDs, banking details, and authorized signatories",
        "requirements": [
          "Certificate of Incorporation",
          "Tax ID / EIN documentation",
          "Authorized signatory list",
          "Banking details for payments",
          "Legal Entity Identifier (LEI)"
        ],
        "effort": "2-5 business days",
        "owner": "Client Onboarding Team"
      },
      "subWorkflow": "dataCollection"
    },
    {
      "id": "kyc",
      "type": "manual",
      "label": "KYC/AML Verification",
      "icon": "shield",
      "details": {
        "description": "Perform Know Your Customer and Anti-Money Laundering checks against regulatory databases and sanctions lists",
        "requirements": [
          "Ultimate Beneficial Owner (UBO) identification",
          "Sanctions screening results",
          "PEP (Politically Exposed Person) check",
          "Adverse media screening"
        ],
        "effort": "1-3 business days",
        "owner": "Compliance Team"
      },
      "subWorkflow": "kycProcess"
    },
    {
      "id": "kyc-decision",
      "type": "gateway",
      "label": "KYC Approved?",
      "icon": "gitBranch",
      "details": {
        "description": "Decision point based on KYC/AML verification results",
        "requirements": ["Completed KYC review", "Risk assessment score"],
        "effort": "Immediate",
        "owner": "Compliance Team"
      }
    },
    {
      "id": "kyc-reject",
      "type": "event",
      "label": "Reject & Notify",
      "icon": "xCircle",
      "details": {
        "description": "Client fails KYC/AML checks - notify relationship manager and document rejection reason",
        "requirements": ["Rejection documentation", "Regulatory filing if required"],
        "effort": "1 business day",
        "owner": "Compliance Team"
      }
    },
    {
      "id": "quality",
      "type": "automated",
      "label": "Data Quality Check",
      "icon": "cog",
      "details": {
        "description": "Automated validation of data completeness, format compliance, and cross-reference verification against external sources",
        "requirements": [
          "All mandatory fields populated",
          "Valid format for tax IDs, LEI",
          "Address verification via external service",
          "Duplicate detection check"
        ],
        "effort": "Automated - 5 minutes",
        "owner": "System"
      }
    },
    {
      "id": "quality-decision",
      "type": "gateway",
      "label": "Quality Pass?",
      "icon": "gitBranch",
      "details": {
        "description": "Automated decision based on data quality validation results",
        "requirements": ["Quality score threshold met", "No critical errors"],
        "effort": "Immediate",
        "owner": "System"
      }
    },
    {
      "id": "quality-fix",
      "type": "manual",
      "label": "Remediate Data Issues",
      "icon": "pencil",
      "details": {
        "description": "Manual correction of data quality issues identified during automated validation",
        "requirements": ["Error report from quality check", "Source documents for correction"],
        "effort": "1-2 business days",
        "owner": "Data Steward"
      }
    },
    {
      "id": "approve",
      "type": "manual",
      "label": "Manager Approval",
      "icon": "checkCircle",
      "details": {
        "description": "Final review and approval by Operations Manager before committing to master data",
        "requirements": [
          "All validations passed",
          "Complete documentation package",
          "Risk assessment reviewed"
        ],
        "effort": "1 business day",
        "owner": "Operations Manager"
      }
    },
    {
      "id": "master",
      "type": "automated",
      "label": "Create Master Record",
      "icon": "database",
      "details": {
        "description": "Automated creation of golden record in Master Data Management (MDM) system with full audit trail",
        "requirements": [
          "Unique client identifier generation",
          "Data lineage tracking",
          "Version control initialization"
        ],
        "effort": "Automated - 2 minutes",
        "owner": "System"
      }
    },
    {
      "id": "distribute",
      "type": "automated",
      "label": "Distribute to Systems",
      "icon": "share2",
      "details": {
        "description": "Automated distribution of client data to downstream consuming systems (CRM, Trading, Billing, etc.)",
        "requirements": [
          "System integration mappings",
          "Data transformation rules",
          "Delivery confirmation from each system"
        ],
        "effort": "Automated - 15 minutes",
        "owner": "System"
      },
      "subWorkflow": "distribution"
    },
    {
      "id": "notify",
      "type": "automated",
      "label": "Notify Stakeholders",
      "icon": "mail",
      "details": {
        "description": "Send automated notifications to relationship manager, client team, and relevant business units",
        "requirements": ["Stakeholder distribution list", "Notification templates"],
        "effort": "Automated - immediate",
        "owner": "System"
      }
    },
    {
      "id": "active",
      "type": "event",
      "label": "Client Active",
      "icon": "checkCircle",
      "details": {
        "description": "Client static data is now active and available for business operations",
        "requirements": [],
        "effort": "N/A - Status milestone",
        "owner": "N/A"
      }
    },
    {
      "id": "maintenance",
      "type": "manual",
      "label": "Ongoing Maintenance",
      "icon": "refreshCw",
      "details": {
        "description": "Periodic review and updates to client static data based on client requests or regulatory requirements",
        "requirements": [
          "Change request documentation",
          "Updated source documents",
          "Approval workflow for material changes"
        ],
        "effort": "Ongoing - as needed",
        "owner": "Data Steward"
      },
      "subWorkflow": "maintenance"
    },
    {
      "id": "review",
      "type": "automated",
      "label": "Annual Review Trigger",
      "icon": "calendar",
      "details": {
        "description": "Automated annual review trigger to ensure client data remains current and compliant",
        "requirements": ["Review schedule configuration", "Notification rules"],
        "effort": "Automated trigger",
        "owner": "System"
      }
    },
    {
      "id": "offboard-decision",
      "type": "gateway",
      "label": "Continue Relationship?",
      "icon": "gitBranch",
      "details": {
        "description": "Decision point during annual review - continue client relationship or initiate offboarding",
        "requirements": ["Relationship status review", "Business justification"],
        "effort": "Decision point",
        "owner": "Relationship Manager"
      }
    },
    {
      "id": "archive",
      "type": "automated",
      "label": "Archive & Retain",
      "icon": "archive",
      "details": {
        "description": "Archive client data per retention policy - typically 7 years for regulatory compliance",
        "requirements": [
          "Retention policy compliance",
          "Audit trail preservation",
          "Downstream system cleanup"
        ],
        "effort": "Automated - 1 hour",
        "owner": "System"
      }
    }
  ],
  "edges": [
    { "id": "e1", "source": "start", "target": "collect" },
    { "id": "e2", "source": "collect", "target": "kyc" },
    { "id": "e3", "source": "kyc", "target": "kyc-decision" },
    { "id": "e4", "source": "kyc-decision", "target": "kyc-reject", "label": "No" },
    { "id": "e5", "source": "kyc-decision", "target": "quality", "label": "Yes" },
    { "id": "e6", "source": "quality", "target": "quality-decision" },
    { "id": "e7", "source": "quality-decision", "target": "quality-fix", "label": "No" },
    { "id": "e8", "source": "quality-fix", "target": "quality", "animated": true },
    { "id": "e9", "source": "quality-decision", "target": "approve", "label": "Yes" },
    { "id": "e10", "source": "approve", "target": "master" },
    { "id": "e11", "source": "master", "target": "distribute" },
    { "id": "e12", "source": "distribute", "target": "notify" },
    { "id": "e13", "source": "notify", "target": "active" },
    { "id": "e14", "source": "active", "target": "maintenance" },
    { "id": "e15", "source": "maintenance", "target": "review" },
    { "id": "e16", "source": "review", "target": "offboard-decision" },
    { "id": "e17", "source": "offboard-decision", "target": "maintenance", "label": "Yes" },
    { "id": "e18", "source": "offboard-decision", "target": "archive", "label": "No" }
  ],
  "subWorkflows": {
